{

    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/media/(.*)",
            "destination": "/media/$1"
        },
        {
            "source": "/form/((?!general).*)",
            "destination": "/form$1"
        },
        {
            "source": "/.*",
            "destination": "/index.html"
        }
    ],

    "git": {
        "deploymentEnabled": {
            "main": true
        }
    }

}

